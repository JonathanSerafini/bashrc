
# Prepend coreutils to standard path
PATH=$(path.prepend "/usr/local/opt/coreutils/libexec/gnubin" "$PATH")

# Prepend rbenv to standard path
PATH=$(path.prepend "~/.rbenv/bin" "$PATH")

# Prepend bashrc supplied scripts
PATH=$(path.prepend "$(bashrc.repo.path bin)" "$PATH")

# Prepend local override supplied scripts
PATH=$(path.prepend "$(bashrc.local.path bin)" "$PATH")

export PATH

